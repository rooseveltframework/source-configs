{
  "timeout": {
    "commandLineArg": "--timeout",
    "envVar": "TIMEOUT",
    "desc": "timeout in ms",
    "default": 5000
  },
  "apiRoute": {
    "commandLineArg": "--api-route",
    "envVar": "API_ROUTE",
    "desc": "base route for API",
    "default": "/api/"
  },
  "exBool": {
    "commandLineArg": "--ex-bool",
    "envVar": "EX_BOOL",
    "desc": "example boolean",
    "default": false
  },
  "exString": {
    "commandLineArg": "--ex-string",
    "envVar": "EX_STRING",
    "desc": "example string",
    "default": "String"
  },
  "commandLineArgArray": {
    "commandLineArg": ["--arg-array", "-a"],
    "desc": "Example array of command line args",
    "default": ""
  },
  "envVarArray": {
    "envVar": ["FOO_BAR", "FOO"],
    "desc": "Example array of environment variables",
    "default": ""
  },
  "httpMethod": {
    "commandLineArg": "--http-method",
    "desc": "example enum",
    "default": "http",
    "values": ["http", "https"]
  },
  "enumWithoutDefault": {
    "commandLineArg": "--no-default",
    "desc": "enum with no default",
    "values": ["yes", "no"]
  },
  "stringWithoutDefault": {
    "commandLineArg": ["--string-no-default", "--swd"],
    "desc": "string with no default"
  },
  "webUrlObj": {
    "httpMethod": {
      "commandLineArg": "--web-http-method",
      "desc": "http method",
      "default": "http"
    },
    "host": {
      "commandLineArg": "--web-host",
      "desc": "web host url",
      "default": "localhost"
    },
    "port": {
      "commandLineArg": "--web-port",
      "desc": "web port",
      "default": 8081
    }
  },
  "sensitiveInfo": {
    "envVar": "SENSITIVE_INFO",
    "desc": "sensitive info",
    "default": "default password",
    "secret": true
  }
}
