{
  "postgresDB": {
    "host": {
      "envVar": "POSTGRES_DB_HOST",
      "desc": "host url for postgres database",
      "default": "localhost"
    },
    "port": {
      "envVar": "POSTGRES_DB_PORT",
      "desc": "port for postgres database",
      "default": 5432
    },
    "user": {
      "envVar": "POSTGRES_DB_USER",
      "desc": "username for app to login with",
      "default": "postgres"
    },
    "password": {
      "envVar": "POSTGRES_DB_PASS",
      "desc": "password for app user login",
      "default": "postgres"
    },
    "database": {
      "envVar": "POSTGRES_DB_DBNAME",
      "desc": "database name for app",
      "default": "postgres"
    }
  },
  "websocket": {
    "host": {
      "envVar": "WS_HOST",
      "desc": "websocket host url",
      "default": "localhost"
    },
    "port": {
      "envVar": "WS_PORT",
      "desc": "websocket port",
      "default": 6400
    },
    "protocol": {
      "envVar": "WS_PROTOCOL",
      "desc": "websocket protocol",
      "default": "ws",
      "acceptedValues": {
        "values": [
          "ws",
          "wss"
        ],
        "fallback": "ws"
      }
    }
  },
  "time": {
    "hour": {
      "commandLineArg": "hour",
      "envVar": "HOUR",
      "desc": "hour",
      "default": 12
    },
    "minute": {
      "envVar": "MINUTE",
      "desc": "minute",
      "default": 30
    },
    "format": "user defined function"
  }
}